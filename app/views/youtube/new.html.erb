

 <div class="container">
  <%= form_tag('/youtube') do %>
    <div class="field">
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
    <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
  <% end %>
</div>